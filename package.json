{
  "name": "rockpack",
  "version": "4.5.2",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "book",
    "next/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "lerna run lint",
    "format": "lerna run format",
    "production": "lerna run production",
    "test": "lerna run test --stream"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint",
    "pre-push": "npm run lint && npm test"
  },
  "dependencies": {
    "caniuse-lite": "1.0.30001653"
  },
  "devDependencies": {
    "lerna": "3.22.1",
    "simple-git-hooks": "2.4.1"
  }
}
