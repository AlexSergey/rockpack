{
  "name": "node-starter",
  "version": "6.0.2",
  "description": "",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "analyzer": "node scripts.build --analyzer",
    "build": "node scripts.build --mode=production",
    "db": "npm run migrate && npm run seed && npm run fixtures",
    "db:production": "npm run migrate:production && npm run seed:production",
    "db:test": "npm run migrate:test && npm run seed:test && npm run fixtures:test",
    "fixtures": "ts-node -P tsconfig.json scripts.fixtures.ts",
    "fixtures:test": "cross-env NODE_ENV=test ts-node -P tsconfig.json scripts.fixtures.ts --env test",
    "format": "npm run format:eslint",
    "format:eslint": "eslint . --fix",
    "lint": "eslint .",
    "migrate": "sequelize db:migrate",
    "migrate:production": "sequelize db:migrate --env production",
    "migrate:test": "cross-env NODE_ENV=test sequelize db:migrate --env test",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "seed:production": "sequelize db:seed:all --env production",
    "seed:test": "cross-env NODE_ENV=test sequelize db:seed:all --env test",
    "start": "node scripts.build",
    "start:prod": "node dist/index.js",
    "test": "cross-env NODE_ENV=test ts-node -P tsconfig.json scripts.dropdatabase.ts && cross-env NODE_ENV=test npm run db:test && node scripts.tests.js",
    "test:watch": "cross-env NODE_ENV=test ts-node -P tsconfig.json scripts.dropdatabase.ts && cross-env NODE_ENV=test npm run db:test && node scripts.tests.js --watch"
  },
  "dependencies": {
    "@koa/cors": "3.4.3",
    "@koa/multer": "3.0.2",
    "@koa/router": "13.1.0",
    "bcrypt": "5.1.1",
    "debug": "4.3.7",
    "dotenv-safe": "8.2.0",
    "friendly-di": "1.0.0",
    "jsonwebtoken": "9.0.2",
    "koa": "2.15.3",
    "koa-bodyparser": "4.4.1",
    "koa-logger": "3.2.1",
    "koa-mount": "4.0.0",
    "koa-static": "5.0.0",
    "multer": "1.4.5-lts.1",
    "mysql2": "2.2.5",
    "pino": "9.5.0",
    "pino-pretty": "13.0.0",
    "pretty-error": "4.0.0",
    "reflect-metadata": "0.2.2",
    "sanitize-html": "2.13.1",
    "sequelize": "5.21.8",
    "sequelize-cli": "6.2.0",
    "sharp": "0.33.5",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@rockpack/codestyle": "6.0.2",
    "@rockpack/compiler": "6.0.2",
    "@rockpack/tester": "6.0.2",
    "@types/dotenv-safe": "8.1.6",
    "@types/enzyme": "3.10.18",
    "@types/jsonwebtoken": "9.0.7",
    "@types/koa": "2.15.0",
    "@types/koa-bodyparser": "4.3.12",
    "@types/koa-mount": "4.0.5",
    "@types/koa-router": "7.4.8",
    "@types/koa-static": "4.0.4",
    "@types/node": "22.9.3",
    "@types/sequelize-fixtures": "0.6.6",
    "@types/validator": "13.12.2",
    "sequelize-fixtures": "1.2.0",
    "ts-node": "10.9.2"
  }
}
